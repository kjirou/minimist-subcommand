{
  "name": "minimist-subcommand",
  "version": "3.0.0",
  "description": "A simple sub-command parser for minimist",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "$(npm bin)/tsc --project ./tsconfigs/build.json",
    "build-and-publish": "rm -r ./dist && npm run build && npm publish",
    "test": "$(npm bin)/mocha --require './setup/ts-node-reigister-for-test.js' './test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjirou/minimist-subcommand.git"
  },
  "keywords": [
    "minimist",
    "getopt",
    "command",
    "parser"
  ],
  "author": "kjirou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kjirou/minimist-subcommand/issues"
  },
  "homepage": "https://github.com/kjirou/minimist-subcommand#readme",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/tv4": "^1.2.29",
    "minimist": "1.2.0",
    "mocha": "^5.0.5",
    "ts-node": "^7.0.1",
    "tv4": "^1.3.0",
    "typescript": "^3.2.2"
  },
  "files": [
    "LICENSE",
    "README.md",
    "examples",
    "index.js",
    "package.json",
    "test"
  ]
